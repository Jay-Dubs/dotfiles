[color]
  ui = true
[alias]
  s = status
  ci = commit
  b = branch -v
  plr = pull --rebase
  d = diff
  co = checkout
  lg = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative
  head = !git --no-pager log origin/instant..HEAD --abbrev-commit --pretty=oneline
  headm = !git --no-pager log origin/master..HEAD --abbrev-commit --pretty=oneline
  find = !sh -c 'git ls-tree -r --name-only HEAD | grep --color $1' -
  cleanb = !git branch --merged instant | grep -v 'instant$' | xargs git branch -d
  cleanbm = !git branch --merged master | grep -v 'master$' | xargs git branch -d
[merge]
  keepBackup = false
  prompt = false
  #tool = p4merge
[user]
  name = Jeremy Wright
  email = jeremywright@goinstant.com
